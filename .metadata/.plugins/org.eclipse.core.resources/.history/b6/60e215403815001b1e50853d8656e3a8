package br.com.ecommerce.beans;


//Pessoafisica é uma classe filha da classe pai Cliente

// tem que ser simples, PessoaFisica **é** Cliente --- herança
// tem composição, Cliente **tem** Endereço

// Cliente é Endereço ... errado, nao tem herança

// interface vai orientar sobre classe filha

public class PessoaFisica {
	
	private String cpf;
	private String rg;
	public String getCpf() {
		return cpf;
	}
	public void setCpf(String cpf) {
		this.cpf = cpf;
	}
	public String getRg() {
		return rg;
	}
	public void setRg(String rg) {
		this.rg = rg;
	}
	
	
	
	

}
